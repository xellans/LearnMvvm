<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:vm="clr-namespace:ViewModel;assembly=ViewModel"
                    xmlns:local="clr-namespace:LearnMvvm"
                    xmlns:userControls="clr-namespace:LearnMvvm.UserControls"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <DataTemplate DataType="{x:Type vm:AuthVM}">
        <local:AuthUserControl />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:PeopleVM}">
        <userControls:PeopleInfo />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProductVM}">
        <userControls:ProductUserControl />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SettingVM}">
        <userControls:SettingUserControl />
    </DataTemplate>
</ResourceDictionary>